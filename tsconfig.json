{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2017",
    "module": "commonjs",
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "*"
      ]
    },
    "removeComments": true, 
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
